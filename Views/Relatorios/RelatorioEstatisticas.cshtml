<div>
    <h2>Relatório Estatístico da Matrícula</h2>
</div>
<hr />
<table>
    <tr>
        <div "margin-top: 1em;">
            <h4>Total de Alunos Matriculados: @ViewData["TotalMatriculados"]</h4>
        </div>
        <hr />
    </tr>
    <tr>
        <h4>Média de Créditos Matriculados: @ViewData["MediaCreditos"]</h4>
        <hr />
    </tr>
</table>
<div>
    <form>
        <input type="submit" value="Imprimir"
               class="btn btn-info btn-sm" asp-controller="" asp-action="" asp-route-TurmaId="" onClick="window.print()" /> |
        <a asp-controller="Home" asp-action="Index">Retornar</a>
    </form>
</div>
